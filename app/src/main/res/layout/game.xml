<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#19F7FF">

    <View
        android:id="@+id/colored_bar"
        android:layout_width="403dp"
        android:layout_height="68dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:background="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/movesText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/remainingText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="224dp"
        android:layout_marginTop="72dp"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/colored_bar2"
        android:layout_width="246dp"
        android:layout_height="111dp"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="52dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/colored_bar3"
        android:layout_width="424dp"
        android:layout_height="420dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="144dp"
        android:background="#E3D6D6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/card01"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="150dp"
        android:onClick="onButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card02"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="150dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card01"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card03"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="150dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card02"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card04"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="150dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card03"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card05"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card01"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card06"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card05"
        app:layout_constraintTop_toBottomOf="@+id/card02"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card07"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card06"
        app:layout_constraintTop_toBottomOf="@+id/card03"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card08"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card07"
        app:layout_constraintTop_toBottomOf="@+id/card04"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card09"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card05"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card10"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card09"
        app:layout_constraintTop_toBottomOf="@+id/card06"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card11"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card10"
        app:layout_constraintTop_toBottomOf="@+id/card07"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card12"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card11"
        app:layout_constraintTop_toBottomOf="@+id/card08"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card13"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card09"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card14"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card13"
        app:layout_constraintTop_toBottomOf="@+id/card10"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card15"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card14"
        app:layout_constraintTop_toBottomOf="@+id/card11"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/card16"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onButton"
        app:layout_constraintStart_toEndOf="@+id/card15"
        app:layout_constraintTop_toBottomOf="@+id/card12"
        app:srcCompat="@drawable/pexeso" />

    <ImageView
        android:id="@+id/soundButton"
        android:layout_width="94dp"
        android:layout_height="126dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="568dp"
        android:onClick="toggleSound"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/soundon" />

    <ImageView
        android:id="@+id/endGameButton"
        android:layout_width="94dp"
        android:layout_height="126dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="568dp"
        android:onClick="onEndGameButton"
        app:layout_constraintStart_toEndOf="@+id/soundButton"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/endgame" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="74dp"
        android:layout_height="106dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>